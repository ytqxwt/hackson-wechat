.selection1 {
    padding-top: 20rpx;
    height: 220rpx;
    width: 100%;
    font-size: 12px;
    color: #ffffff;
    background: #ffab32;
}

.selection1 .s1-left {
    height: 150rpx;
    width: 33.3%;
    text-align: center;
    float: left;
    position: relative;
}

.s1-left view {
    position: relative;
    bottom: 0rpx;
    margin: 78rpx auto;
}

.selection1 .s1-mid {
    display: flex;
    width: 33.3%;
    flex-direction: column;
    justify-content: center;
    color: #ffffff;
    float: left;
    text-align: center;
}

.selection1 .mid-img {
    height: 50rpx;
    width: 50rpx;
    border: 1px solid #ffffff;
    border-radius: 50%;
    margin: 0 auto;
    margin-bottom: 20rpx;
}
.userinfo {  
    position: relative;  
    width: 750rpx;  
    height: 320rpx;  
    color: #fff;  
    display: flex;  
    flex-direction: column;  
    align-items: center;  
  }  
    
  .userinfo-avatar {  
    overflow:hidden;  
    display: block;  
    width: 160rpx;  
    height: 160rpx;  
    margin: 20rpx;  
    margin-top: 50rpx;  
    border-radius: 50%;  
    border: 2px solid #fff;  
    box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);  
  }  
    
  .userinfo text {  
    margin-left: 100rpx;
    /* color: #fff;   */
    font-size: 14px;  
    background-color: #c0c0c0;  
    border-radius:40%;  
  }  
.selection1 .s1-right {
    height: 150rpx;
    width: 33.3%;
    display: flex;
    flex-direction: column;
    float: left;
    position: relative;
}

.s1-right view {
    position: absolute;
    bottom: -16rpx;
    margin-left: 70rpx;
}

radio {
    transform: scale(0.5);
}

.selection2 {
    height: 250rpx;
    width: 600rpx;
    background: white;
    margin: 50rpx auto;
    box-shadow: 10rpx 10rpx 50rpx 2rpx #bbbaba;
    border-radius: 10px;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    padding: 0rpx 44rpx;
}

.selection2 navigator {
    display: inline-block;
}

.swiper-box {
    display: block;
    height: 100%;
    width: 100%;
    overflow: hidden;
}

.swiperItems {
    height: 150rpx;
    width: 25%;
    margin-top: 30rpx;
    font-size: 12px;
    color: #9e9a9a;
    display: flex;
    flex-direction: column;
}

.page {
    position: absolute;
    bottom: 0rpx;
    width: 120rpx;
    height: 50rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    margin-left: 243rpx;
}

.dot1,
.dot2,
.dot3 {
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    margin-top: 10rpx;
    background: #b2b2b2;
}

.itemView {
    height: 120rpx;
    width: 80%;
    border-radius: 50%;
    text-align: center;
}

.itemText {
    width: 80%;
    text-align: center;
}

.sign-img {
    background: #6fcda8;
}

.message-img {
    background: #74cbf8;
}

.grades-img {
    background: #fcc56a;
}

.memorandum-img {
    background: #ffc5bb;
}

.setting-img {
    background: #bababa;
}

.lesson-img {
    background: #a09fed;
}

.selection3 {
    width: 690rpx;
    background: white;
    margin: 0 auto;
    box-shadow: 10rpx 10rpx 50rpx 2rpx #bbbaba;
    border-radius: 10px;
}

.selection3 .top {
    text-align: center;
    line-height: 100rpx;
    height: 80rpx;
    width: 600rpx;
    font-size: 12px;
    color: #bbbbbb;
    border: #6389d0;
    margin: 0rpx auto;
    border-bottom: 1px solid #6389d0;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.tomorrow {
    color: #bbbbbb;
}

.selection3 .top #line {
    color: #6389d0;
}

.bottom {
    width: 600rpx;
    margin: 20rpx auto;
    font-size: 12px;
    color: #666666;
    text-align: center;
}

.selection4 {
    width: 100%;
    margin-top: 50rpx;
}

.selection4-content {
    width: 690rpx;
    background: white;
    margin: 0 auto;
    box-shadow: 10rpx 10rpx 50rpx 2rpx #bbbaba;
    border-radius: 10px;
}

.selection4-content .tittle {
    height: 90rpx;
    line-height: 100rpx;
    width: 500rpx;
    color: #ff6732;
    font-size: 12px;
    margin: 0 auto;
    text-align: center;
    border-bottom: 1px solid #ff6732;
}
.table {
  margin-top: 10rpx;
   border: 0px solid darkgray; 
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 80rpx;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
    color: #666666;
}
.bg-g{
  background: #ffab32;
}
.th {
  width: 40%;
  justify-content: center;
   background: #ffab32; 
  color: #fffffc;
  display: flex;
  height: 70rpx;
  align-items: center;
  font-size: 12px;
}
.date,.text{
  display: inline-block;
  margin-left: 20rpx;
}
.selection5 {
    height: 400rpx;
    width: 600rpx;
    background: white;
    margin: 50rpx auto;
    box-shadow: 10rpx 10rpx 50rpx 2rpx #bbbaba;
    border-radius: 10px;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    padding: 0rpx 44rpx;
}

.ads {
    height: 200rpx;
    width: 600rpx;
    background: #77c9d4;
}